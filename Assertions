describe('My First Test', () => {
  it('clicking "type" shows the right headings', () => {
    // Mengunjungi halaman pertama
    cy.visit('https://example.cypress.io');
    cy.wait(2000); // Ganti angka 2000 dengan jumlah milidetik yang diinginkan
    cy.contains('type').click();
    cy.url().should('include', '/commands/actions');
    cy.wait(1000);
    cy.get('.action-email').type('example@mail.com').should('have.value', 'example@mail.com');
  });
});
